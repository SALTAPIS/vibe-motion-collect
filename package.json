    "cors": "^2.8.5",
    "three": "^0.158.0",
    "gsap": "^3.12.2",
    "lil-gui": "^0.19.1",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "simple-git": "^3.20.0",
    "fs-extra": "^11.1.1",
    "chokidar": "^3.5.3",
    "marked": "^9.1.6",
    "yaml": "^2.3.4"
  },
  "keywords": [
    "threejs",
    "gsap",
    "webgl",
    "animation",
    "gallery",
    "examples"
