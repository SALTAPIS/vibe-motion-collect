{
  "name": "vibe-motion-collect",
  "version": "1.0.0",
  "description": "A curated collection of animation examples using Three.js, GSAP, and other web animation libraries",
  "main": "index.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "fetch": "node scripts/fetch-examples.js",
    "build-gallery": "node scripts/build-gallery.js",
    "setup-examples": "node scripts/setup-examples.js",
    "setup": "npm install && npm run fetch && npm run setup-examples && npm run build-gallery"
  },
  "dependencies": {
